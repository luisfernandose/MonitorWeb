@model Queue.Models.Agent_EmployeesGroups

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@using (Html.BeginForm("Create", "Agent_EmployeesGroups", FormMethod.Post, new { id = "frm_register" }))
{
    @Html.AntiForgeryToken()

    <div id="divblock" class="row">
        <div class="col-lg-12 col-12">

            <div class="box">
                <div class="box-header with-border">
                    <h4 class="box-title">Crear Grupo Usuarios</h4>
                </div>
                <div class="col-md-2">
                    <a href="@Url.Action("Index", "Agent_EmployeesGroups")" class="btn btn-info btn-block m-t-10 float-right">
                        <i class="glyphicon glyphicon-chevron-left"></i> Back
                    </a>
                </div>

                <div class="box-body">

                    <div class="form-group">
                        <div class="row">
                            <div class="col-lg-4">
                                @Html.LabelFor(model => model.Nombre, htmlAttributes: new { @class = "control-label" })
                                @Html.ValidationMessageFor(model => model.Nombre, "", new { @class = "text-danger" })
                                <div class="input-group">
                                    @Html.EditorFor(model => model.Nombre, new { htmlAttributes = new { @class = "form-control required" } })
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="form-actions">
                    <div class="d-flex flex-row-reverse">
                        <div class="p-2">
                            <button id="add_company" name="add_dictionary" type="submit" value="Create" class="btn btn-info waves-effect waves-light btn-block">
                                Add
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}

